<project type="egg">
  <deps purpose="run">
    <egg name="reahl-component"/>
    <egg name="reahl-web"/>
    <egg name="reahl-sqlalchemysupport"/>
    <egg name="reahl-web-declarative"/>
  </deps>
  <alias name="unit" command="setup -- check"/>
  <persisted>
    <class locator="reahl.doc.examples.tutorial.addresslistdb.addresslistdb:Address"/>
  </persisted>
  <alias name="unit" command="setup -- -q pytest --addopts '-s --pyargs reahl.doc.examples.tutorial.addresslistdb.addresslistdb_dev'"/>
  <alias name="demosetup" command="setup -- -q pytest --addopts '-s -o python_functions=demo_setup --pyargs reahl.doc.examples.tutorial.addresslistdb.addresslistdb_dev'"/>
</project>
